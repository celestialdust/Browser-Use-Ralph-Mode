# Browser Use Agent Environment Configuration
# Copy this file to .env and fill in your credentials

# =============================================================================
# OpenAI API (Recommended)
# =============================================================================
USE_AZURE=false
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-5

# =============================================================================
# Azure OpenAI (Deprecated - use OpenAI API instead)
# =============================================================================
# USE_AZURE=true
# AZURE_OPENAI_ENDPOINT=https://your-endpoint.openai.azure.com
# AZURE_OPENAI_API_KEY=your-azure-api-key-here
# OPENAI_API_VERSION=2025-01-01-preview
# DEPLOYMENT_NAME=your-gpt-deployment-name

# =============================================================================
# Model Settings
# =============================================================================
TEMPERATURE=1.0

# Reasoning API (for models that support it)
REASONING_ENABLED=true
REASONING_EFFORT=medium
REASONING_SUMMARY=detailed

# =============================================================================
# Browser Streaming
# =============================================================================
AGENT_BROWSER_STREAM_PORT=9223

# =============================================================================
# Chrome DevTools Protocol (optional - connect to existing browser)
# =============================================================================
# USE_CDP=true
# CDP_PORT=9222

# =============================================================================
# LangGraph Server
# =============================================================================
LANGGRAPH_PORT=2024

# =============================================================================
# Browserbase (Cloud Browser - optional)
# For serverless deployments where local browser isn't available
# Get credentials from https://browserbase.com/overview
# =============================================================================
# BROWSERBASE_API_KEY=your-browserbase-api-key
# BROWSERBASE_PROJECT_ID=your-browserbase-project-id
